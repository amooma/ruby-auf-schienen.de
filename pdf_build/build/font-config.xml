<fop version="1.0">
	<renderers>
		<renderer mime="application/pdf">
			<fonts>
				<font metrics-url="build/fonts/DejaVuSansMono.xml" kerning="yes" embed-url="build/fonts/DejaVuSansMono.ttf">
					<font-triplet name="DejaVuSansMono" style="normal" weight="normal"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSansMono-Bold.xml" kerning="yes" embed-url="build/fonts/DejaVuSansMono-Bold.ttf">
					<font-triplet name="DejaVuSansMono" style="normal" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSansMono-BoldOblique.xml" kerning="yes" embed-url="build/fonts/DejaVuSansMono-BoldOblique.ttf">
					<font-triplet name="DejaVuSansMono" style="italic" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSansMono-Oblique.xml" kerning="yes" embed-url="build/fonts/DejaVuSansMono-Oblique.ttf">
					<font-triplet name="DejaVuSansMono" style="italic" weight="normal"/>
				</font>
				
				<font metrics-url="build/fonts/DejaVuSans.xml" kerning="yes" embed-url="build/fonts/DejaVuSans.ttf">
					<font-triplet name="DejaVuSans" style="normal" weight="normal"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSans-Bold.xml" kerning="yes" embed-url="build/fonts/DejaVuSans-Bold.ttf">
					<font-triplet name="DejaVuSans" style="normal" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSans-BoldOblique.xml" kerning="yes" embed-url="build/fonts/DejaVuSans-BoldOblique.ttf">
					<font-triplet name="DejaVuSans" style="italic" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/DejaVuSans-Oblique.xml" kerning="yes" embed-url="build/fonts/DejaVuSans-Oblique.ttf">
					<font-triplet name="DejaVuSans" style="italic" weight="normal"/>
				</font>


				<font metrics-url="build/fonts/Georgia.xml" kerning="yes" embed-url="build/fonts/Georgia.ttf">
					<font-triplet name="Georgia" style="normal" weight="normal"/>
				</font>
				<font metrics-url="build/fonts/Georgia_Bold.xml" kerning="yes" embed-url="build/fonts/Georgia_Bold.ttf">
					<font-triplet name="Georgia" style="normal" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/Georgia_Italic.xml" kerning="yes" embed-url="build/fonts/Georgia_Italic.ttf">
					<font-triplet name="Georgia" style="italic" weight="normal"/>
				</font>
				<font metrics-url="build/fonts/Georgia_Bold_Italic.xml" kerning="yes" embed-url="build/fonts/Georgia_Bold_Italic.ttf">
					<font-triplet name="Georgia" style="italic" weight="bold"/> 
				</font>

				<font metrics-url="build/fonts/MetaPlusNormal-Roman.xml" kerning="yes" embed-url="build/fonts/MetaPlusNormal-Roman.ttf">
					<font-triplet name="MetaPlusNormal-Roman" style="normal" weight="normal"/>
				</font>
				<font metrics-url="build/fonts/MetaPlusBold-Roman.xml" kerning="yes" embed-url="build/fonts/MetaPlusBold-Roman.ttf">
					<font-triplet name="MetaPlusBold" style="normal" weight="bold"/>
				</font>
				<font metrics-url="build/fonts/MetaPlusBold-Italic.xml" kerning="yes" embed-url="build/fonts/MetaPlusBold-Italic.ttf">
					<font-triplet name="MetaPlusBold-Italic" style="italic" weight="bold"/> 
				</font>

			</fonts>
		</renderer>
	</renderers>
</fop>
